<script>
  function toggle_usa_banner() {
    var usa_banner_state = jQuery('section.usa-banner .usa-banner-content').attr('aria-expanded');

    if( usa_banner_state == 'false' ) {
      // Update state
      jQuery('section.usa-banner .usa-banner-content').attr('aria-expanded', 'true');

      // Show
      jQuery('section.usa-banner .usa-banner-content').css('display', 'block');

      // Update arrow
      jQuery('section.usa-banner header.usa-banner-header button.usa-accordion-button').removeClass('accordion-state-closed');
      jQuery('section.usa-banner header.usa-banner-header button.usa-accordion-button').addClass('accordion-state-open');
    } else {
      // Update state
      jQuery('section.usa-banner .usa-banner-content').attr('aria-expanded', 'false');

      // Show
      jQuery('section.usa-banner .usa-banner-content').css('display', 'none');

      // Update arrow
      jQuery('section.usa-banner header.usa-banner-header button.usa-accordion-button').removeClass('accordion-state-open');
      jQuery('section.usa-banner header.usa-banner-header button.usa-accordion-button').addClass('accordion-state-closed');
    }

  }

